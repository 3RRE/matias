@using S3k.Utilitario;

@{
    ViewBag.Title = "Registro de Progresivo";
    Layout = "~/Views/Shared/_Layout.cshtml";

    string fecha_archivos = ClaseEncriptacion.EncriptarCadenaHASH(Convert.ToString(DateTime.Now));
}

@section css
{
    <style>
        .header-boxed {
            width: 100%;
            position: relative;
            background-color: #EEEEEE;
            padding: 10px;
            margin-bottom: 15px;
            border-radius: 5px !important;
            overflow: hidden;
        }

        .headerbx-info {
            position: relative;
            margin-bottom: 10px;
        }

        .headerbx-actions {
            position: relative;
        }

        .form-box {
            width: 100%;
            position: relative;
            background-color: #EEEEEE;
            padding: 10px;
            border-radius: 5px !important;
            overflow: hidden;
        }

        .form-box-item {
            margin-bottom: 5px;
        }

        .form-border {
            border: 1px solid #4a85b7 !important;
            height: auto !important;
        }

        .table-history {
            width: 100%;
            position: relative;
            background-color: #FFFFFF;
            border-spacing: 0;
            border-radius: 5px !important;
            overflow: hidden;
        }

            .table-history th, .table-history td {
                padding: 2px 4px;
            }

            .table-history thead th {
                border: 1px solid rgba(255, 255, 255, 0.25);
            }

            .table-history tbody td {
                border: 1px solid #DDDDDD;
            }

            .table-history tr th {
                text-align: center;
            }

            .table-history tbody td {
                padding: 4px 4px;
            }

            .table-history tbody tr:nth-child(even) {
                background-color: #FFFFFF;
            }

            .table-history tbody tr:nth-child(odd) {
                background-color: #F2F2F2;
            }

            .table-history thead tr th {
                background-color: #00226C;
                color: #FFFFFF;
            }

            .table-history tbody tr:hover {
                background-color: #DDDDDD;
            }

        .table-dtable > thead > tr > th, .table-dtable > tbody > tr > td {
            height: auto;
            border-bottom: 0;
            border-top: 0;
        }

        .close-custm {
            color: #EEEEEE;
            opacity: 0.8;
            font-size: 25px;
        }

            .close-custm:hover {
                color: #FFFFFF;
                opacity: 1;
            }

        .header-bg-nwo {
            background-color: #FF8000;
        }

        .headerbx-sala {
            font-size: 14px;
            font-weight: 700;
            color: #484848;
        }

        .headerbx-progresivo {
            font-size: 15px;
            font-weight: 700;
            color: #222222;
        }

        .headerbx-fecha {
            font-size: 14px;
            font-weight: 700;
            color: #263E78;
        }
    </style>
}

