@{
    ViewBag.Title = "Consulta CLiente Juego";
    Layout = "~/Views/Shared/_ConsultaJuego.cshtml";
}
@using System.Activities.Expressions
@using System.Web.Script.Serialization
@using System.Web.UI.WebControls

<style>
    .widget-box {
        margin: 0px !important;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12">
               
                <div class="col-md-7 col-md-offset-2 col-xs-7 col-lg-7">
                    <p>
                        <h2 class="text-center"><b>Consulta Cliente Juego</b></h2>
                    </p>
                    <div style="line-height: 0.9;">
                      
                    </div>
                </div>
              
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 col-xs-12 bg_white">
                <input name="id" id="id" value="0" type="hidden" />
                <div class="widget-box widget-color-dark">
                    <div class="widget-header">
                        <hr />
                    </div>
                    <div class="widget-body">
                        <div class="widget-main">
                            <div class="row">
                                <div class="col-md-3 col-sm-4">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    Nro. Doc.
                                                </span>
                                                <input type="text" class="form-control input-sm" id="nrodoc" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    DEL
                                                </span>
                                                <input type="text" class="form-control input-sm dateOnly_" id="fechaInicio" readonly />
                                                <span class="input-group-addon input-group-icon">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    AL
                                                </span>
                                                <input type="text" class="form-control input-sm dateOnly_" id="fechaFin" readonly />
                                                <span class="input-group-addon input-group-icon">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-4">
                                    <div class="row">
                                        <div class="col-md-12 col-sm-12 col-xs-12">
                                            <button type="button" id="btnBuscar" class="btn btn-success btn-sm col-md-12 col-xs-12">
                                                <span class="fa fa-search"></span>
                                                BUSCAR
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="widget-box widget-color-dark">
                    <div class="widget-header">
                        <hr />
                    </div>
                    <div class="widget-body">
                        <div class="widget-main">
                            <div class="row">
                                <div class="col-md-12">
                                   <table id="tableregistro" style="width:100%" class="table table-bordered table-condensed table-striped">

                                   </table>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
          

            </div>
        </div>
    </div>
</div>
@section js
{
    <script type="text/javascript">
            
    </script>
    <script src="~/Content/assets/viewsjs/Campania/ClienteConsultaJuego.js"></script>
}

