    .roomm-items {
        width: 100%;
        position: relative;
        padding: 15px 15px;
    }

    .roomm-item {
        width: 100%;
        position: relative;
        padding: 15px 15px;
        background-color: #FFEBCD;
        border: 1px solid #A9A9A9;
        border-radius: 4px !important;
    }

    .roomm-item-name {
        font-size: 12px;
        color: #171717;
        font-weight: 700;
        margin-left: 5px;
    }

    .roomm-item-company {
        display: block;
        color: #484848;
        font-weight: 400;
        font-size: 12px;
        font-style: italic;
        margin-top: 4px;
    }

    .row-m0 {
        margin: 0;
    }

    .row-m-5 {
        margin-left: -5px;
        margin-right: -5px;
    }

    .col-p5 {
        padding: 5px;
    }

    .col-p10 {
        padding: 10px;
    }

    .caaht-actions {
        display: flex;
        align-items: center;
    }

    .caaht-gridd-box, .caaht-count-box {
        line-height: 0;
    }

    .caaht-gridd-box {
        margin-right: 10px;
    }

    .caaht-btn-gridd {
        display: inline-block;
