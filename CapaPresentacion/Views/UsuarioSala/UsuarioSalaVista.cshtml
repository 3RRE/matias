    }

    .block-loaderdata {
        width: 100%;
        position: relative;
        padding: 30px 0;
    }

    .loaderdata {
        width: 48px;
        height: 48px;
        border: 5px solid #FFF;
        border-bottom-color: #143958;
        border-radius: 50% !important;
