<style>
    .caard-my-3 {
        margin-top: 3px;
        margin-bottom: 3px;
    }

    .caard {
        width: 100%;
        position: relative;
        background-color: #FFFFFF;
        transition: 0.3s ease-in-out;
        box-shadow: 0 20px 25px -5px rgb(0 0 0 / 10%), 0 10px 10px -5px rgb(0 0 0 / 4%);
        border-radius: 8px !important;
        overflow: hidden;
    }

    .caard-header {
        width: 100%;
        position: relative;
        padding: 15px 15px;
        border-bottom: 1px solid #DDDDDD;
    }

    .caard-header-top {
        width: 100%;
        position: relative;
        display: flex;
        align-items: center;
    }

        .caard-header-top .caaht-titlebox {
            margin-right: auto;
        }

        .caard-header-top .caaht-countbox {
            margin-left: auto;
        }

    .caard-header-title {
        color: #000000;
        font-size: 16px;
        font-weight: 700;
        margin: 0;
        line-height: 1.25;
    }

    .caard-header-subtitle {
        margin: 0;
        font-size: 13px;
        font-weight: 400;
        color: #0065ED;
        letter-spacing: 1px;
        line-height: 1;
    }

    .caard-header-count {
        color: #444444;
        font-size: 13px;
        font-weight: 400;
    }

    .caard-header-bottom {
        width: 100%;
        position: relative;
        padding: 5px 0;
        margin-top: 8px;
    }

    .caard-footer {
        width: 100%;
        position: relative;
        padding: 10px 15px;
        text-align: center;
        background-color: #F5F5F5;
        border-top: 1px solid #DDDDDD;
    }

    .caard-footer-total {
        display: inline-block;
        text-align: center;
        color: #242424;
        font-size: 14px;
        font-weight: 400;
    }

    .caard-search-wrap {
        width: 100%;
        position: relative;
    }

    .caard-search {
        width: 100%;
        background-color: #F5F5F5;
        position: relative;
        color: #000000;
        font-size: 14px;
        padding: 7px 28px 7px 14px;
        border: 1px solid #DDDDDD !important;
        outline: none !important;
        border-radius: 5px !important;
    }

    .caard-search-icon {
        position: absolute;
        color: #999999;
        font-size: 16px;
        right: 10px;
        top: calc(50% - 8px);
    }

    .caard-search:focus {
        border-color: #1F91EF !important;
    }

        .caard-search:focus ~ .caard-search-icon {
            color: #1F91EF;
        }

    .caard-body {
        width: 100%;
        position: relative;
    }

    .caard-list {
        width: 100%;
        position: relative;
        margin: 0;
        padding: 0;
        list-style: none;
    }

        .caard-list .caard-list-itm {
            width: 100%;
            position: relative;
            display: block;
        }

        .caard-list .list-itm-wrap {
            width: 100%;
            display: flex;
            align-items: center;
            padding: 15px 15px;
            border-top: 1px solid #DDDDDD;
            cursor: pointer;
        }

        .caard-list .caard-list-itm:last-child {
            border-bottom: 1px solid #DDDDDD;
        }

        .caard-list .caard-list-itm:nth-child(odd) {
            background-color: #F5F5F5;
        }

        .caard-list .caard-list-itm:nth-child(even) {
            background-color: #FFFFFF;
        }

        .caard-list .caard-list-itm .litm-namebox {
            margin-right: 10px;
        }

        .caard-list .caard-list-itm .litm-countbox {
            margin-left: auto;
            line-height: 0;
        }

        .caard-list .caard-list-itm .list-itm-name {
            color: #171717;
            font-size: 13px;
            font-weight: 500;
            line-height: 1.5;
        }

        .caard-list .caard-list-itm .list-itm-count {
            color: #171717;
            font-size: 12px;
            font-weight: 400;
            background-color: #FFFFFF;
            padding: 0px 4px;
            line-height: 1.5;
            display: inline-block;
            border: 1px solid #B9B9B9;
            white-space: nowrap;
            overflow: hidden;
            border-radius: 4px !important;
        }

            .caard-list .caard-list-itm .list-itm-count.litm-count-success {
                background-color: #1BA345;
                color: #FFFFFF;
                border: 1px solid #EEEEEE;
            }

            .caard-list .caard-list-itm .list-itm-count.litm-count-warning {
                background-color: #D4881B;
                color: #FFFFFF;
                border: 1px solid #EEEEEE;
            }

            .caard-list .caard-list-itm .list-itm-count.litm-count-danger {
                background-color: #D43443;
                color: #FFFFFF;
                border: 1px solid #EEEEEE;
            }

        .caard-list .caard-list-itm:hover, .caard-list .caard-list-itm.active {
            background-color: #2489C5;
            border-top-color: rgba(245, 245, 245, 0.5);
        }

            .caard-list .caard-list-itm:hover .list-itm-name, .caard-list .caard-list-itm.active .list-itm-name {
                color: #FFFFFF;
            }

    .caard-content-items {
