    .user-container {
        max-width: 700px;
        display: flex;
        gap: 45px;
    }
    @@keyframes rotation {
        0% {
            transform: rotate(0deg);
        }

        100% {
            transform: rotate(360deg);
        }
    }
    @@media (max-width:600px){
        .user-container{
            flex-direction:column;
        }
    }
