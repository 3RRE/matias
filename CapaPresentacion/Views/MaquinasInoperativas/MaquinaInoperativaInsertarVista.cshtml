@section css
{
    <style type="text/css">
        .select2-container .select2-selection--single {
            height: 27px !important;
        }

        .select2-container .select2-selection--multiple {
            min-height: 27px !important;
            max-height: 27px !important;
        }

        .select2-container .select2-selection--multiple {
            overflow-y: scroll;
        }

        .table thead tr {
            background-color: #0b1f46 !important;
            height: 45px;
        }

        .cardMachine {
            border-radius: 7px !important;
            background-color: white;
            padding: 15px 15px;
        }

        .sectionCard {
            border: solid #a1a1a1 1px;
            border-radius: 7px !important;
            padding: 15px 1px;
            margin-bottom: 15px;
        }

        .collapsed {
            box-shadow: none !important;
        }

        .block.collapsed .block-heading .block-controls .icon.block-control-collapse {
            color: black !important;
        }

        .block {
            box-shadow: none !important;
        }

        .zon {
            background-color: white !important;
            border-bottom: none !important;
            background: none !important;
        }

        .infomaq {
            display: flex;
            flex-direction: column;
        }

        .maqContain {
            box-shadow: 0px 1px 5px -1px rgba(128,128,128,0.75);
            padding: 5px 15px;
            border-radius: 5px !important;
            justify-content: center;
            display: flex;
            align-items: center;
            background-color: #81b0ff1f;
            margin-bottom: 7px;
        }

        .titleMaq {
            font-weight: bold;
            color: #05256e;
            justify-content: center;
            display: flex;
        }

        .textMaq {
            color: #6d6d6d;
            text-shadow: none !important;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        @@media (max-width: 500px) {
            .cardMachine {
                padding: 2px;
            }

            .right-column-content {
                padding: 8px !important;
            }

            .input-group-addon {
                font-size: .7rem;
            }
        }
    </style>
}

