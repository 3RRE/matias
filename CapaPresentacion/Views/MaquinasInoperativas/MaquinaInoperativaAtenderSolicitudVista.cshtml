@using System.Activities.Expressions
@using System.Web.Script.Serialization
@using System.Web.UI.WebControls
@using S3k.Utilitario;

@{
    String maquinaInoperativa = new JavaScriptSerializer().Serialize(ViewBag.maquinaInoperativa);
    String listaProblemas = new JavaScriptSerializer().Serialize(ViewBag.listaProblemas);
    String listaCategoriaProblemas = new JavaScriptSerializer().Serialize(ViewBag.listaCategoriaProblemas);
    String listaPiezas = new JavaScriptSerializer().Serialize(ViewBag.listaPiezas);
    String listaRepuestos = new JavaScriptSerializer().Serialize(ViewBag.listaRepuestos);
    String listaProblemasArray = new JavaScriptSerializer().Serialize(ViewBag.listaProblemasArray);

}
<script type="text/javascript">
    maquinaInoperativa = (@Html.Raw(maquinaInoperativa));
    listaProblemas = (@Html.Raw(listaProblemas));
    listaCategoriaProblemas = (@Html.Raw(listaCategoriaProblemas));
    listaPiezas = (@Html.Raw(listaPiezas));
    listaRepuestos = (@Html.Raw(listaRepuestos));
    listaProblemasArray = (@Html.Raw(listaProblemasArray));
</script>

