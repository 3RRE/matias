<div class="row">
    <div class="col-md-12">
        <div class="col-md-6 lineDerecha">
            <table id="tableCargo" cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered compact table-hover table-condensed">
                <thead>
                    <tr>
                        <th style="display: none">Codigo</th>
                        <th>Nombre</th>
                        <th>Estado</th>
                        <th style="display: none">Estado</th>
                        <th>Accion</th>
                    </tr>
                </thead>
                <tbody></tbody>

            </table>
        </div>
        <div class="col-md-6">
            <form id="formCargo" class="form-horizontal notlabel " role="form">

                <div class="form-group">
                    <label for="txtCodigoCargo" class="col-sm-2 control-label">Codigo</label>
                    <div class="col-sm-10">
                        <input type="text" readonly="readonly" class="form-control input-sm" id="txtCodigoCargo" name="CargoID" placeholder="Autogenerado">
                    </div>
                </div>
                <div class="form-group">
                    <label for="txtNombreCargo" class="col-sm-2 control-label">Nombre</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control input-sm" id="txtNombreCargo" name="Descripcion" placeholder="Nombre">
                    </div>
                </div>

                <div class="form-group">
                    <label for="cboEstadoCargo" class="col-sm-2 control-label">Estado </label>
                    <div class="col-sm-10">
                        <select class="form-control input-sm" id="cboEstadoCargo" name="Estado">
                            <option value="">--Seleccione--</option>
                            <option value="0">INACTIVO</option>
                            <option value="1">ACTIVO</option>
                        </select>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

