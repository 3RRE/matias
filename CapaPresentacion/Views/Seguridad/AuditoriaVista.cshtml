                                    <h3 style="color:blue;font-weight:bolder">Data Encontrada en Formulario</h3>
                                    <pre id="dataInicial" style="white-space:normal;min-height:100px;text-align: justify;"></pre>
                                </div>

                                <div id="divfinal">
                                    <h3 style="color:blue;font-weight:bolder">Data Guardada o Enviada para consulta</h3>
                                    <pre id="dataFinal" style="white-space:normal;min-height:100px;text-align: justify;"></pre>
                                </div>*@

                            </div>
                        </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cerrar</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
}



