@using CapaEntidad
@using S3k.Utilitario;
@{
    string fecha_archivos = ClaseEncriptacion.EncriptarCadenaHASH(Convert.ToString(DateTime.Now));
    int id = 0;

    SEG_UsuarioEntidad usuario = (SEG_UsuarioEntidad)Session["usuario"];
    //id = usuario.UsuarioID;

    if (usuario == null)
    {
        Response.Redirect("~/Home/Login");
    }
    else
    {
        id = usuario.UsuarioID;
    }

    string url = $"{Request.Url.Scheme}://{Request.Url.Authority}{Request.ApplicationPath.TrimEnd('/')}";
}

