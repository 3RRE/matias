    <style>
        .header-box {
            width: 100%;
            display: flex;
            align-items: center;
            padding: 5px 25px;
            /* background: #0D0F16;*/
        }

        .h-brand-box {
            position: relative;
        }

        .h-dateclock-box {
            margin-left: 5px;
        }

            .h-dateclock-box .h-date-text, .h-dateclock-box .h-clock-text {
                color: #FFFFFF;
                font-size: 12px;
                line-height: 15px;
            }

        .h-menu-square-box {
            position: relative;
        }

            .h-menu-square-box .navbar-toggle {
                margin: 0;
                background-color: #12141C;
                border-color: rgba(255, 255, 255, 0.5);
                -webkit-transition: all 0.2s;
                -moz-transition: all 0.2s;
                -o-transition: all 0.2s;
                transition: all 0.2s;
                border-radius: 2px !important;
            }

                .h-menu-square-box .navbar-toggle:hover {
                    background-color: #B20000;
                    border-color: #FFFFFF;
                }

        .h-brand-box img {
            width: 130px;
            height: auto;
            max-width: 100%;
        }

        .h-account-box {
            padding: 0 5px;
            margin-left: auto;
        }

            .h-account-box .header-profile {
                display: block;
                margin: 0;
            }

                .h-account-box .header-profile a {
                    line-height: 15px;
                }

                    .h-account-box .header-profile a span.main-menu-text {
                        display: inline-block;
                        padding: 0;
                        text-align: right;
                        margin-right: 5px;
                        max-width: 125px;
