                        display: flex;
                        align-items: center;
                    }

        .h-navbar-toggle-box {
            display: none !important;
        }

        .block h1, .block .h1 {
            display: flex;
            align-items: center;
            margin: 2px 0;
        }

            .block h1 .icon, .block .h1 .icon {
                margin: 0;
                margin-right: 5px;
            }

            .block h1 .main-text, .block .h1 .main-text {
                font-size: 16px;
                font-weight: 700;
                color: #143958;
            }

        @@media (max-width: 767px) {
            .header-box {
                padding: 5px 10px;
            }

            .h-account-box .main-menu-text {
                font-size: 12px;
            }

            .h-dateclock-box {
                margin-left: 10px;
            }

                .h-dateclock-box .h-date-text, .h-dateclock-box .h-clock-text {
                    font-size: 11px;
                }
        }

        @@media (max-width: 480px) {
            .h-account-box .menu-open > ul {
                top: 48px !important;
            }

            .block h1 .main-text, .block .h1 .main-text {
                font-size: 14px;
            }
        }


