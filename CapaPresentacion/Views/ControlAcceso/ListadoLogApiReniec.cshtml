
@{
    ViewBag.Title = "Listado Log Api Reniec";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-6">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <h1>
                                <span aria-hidden="true" class="icon icon-dollar"></span>
                                <span class="main-text">
                                    Listado Log Api Reniec
                                </span>
                            </h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">

                    <div class="row">

                        <div class="col-md-6 col-sm-6 col-xs-6">
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <button type="button" id="btnGraficos" class="btn btn-primary btn-sm col-md-12 col-xs-12">
                                        <span class="glyphicon glyphicon-file"></span>
                                        Graficos
                                    </button>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-6">
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <button type="button" id="btnBuscar" class="btn btn-success btn-sm col-md-12 col-xs-12">
                                        <span class="glyphicon glyphicon-book"></span>
                                        Buscar
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


<div class="row">
    <div class="col-md-12">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row">

                        <div class="col-xs-12 col-md-12 col-sm-12">

                            <div class="row" id="formfiltro">
                                <div class="col-md-4 col-sm-4">
                                    <div class="input-group ">
                                        <span class="input-group-addon">
                                            Tipo
                                        </span>
                                        <select id="cboTipo" class="form-control input-sm" name="Tipo">
                                            <option value="0">Todos</option>
                                            <option value="1">Web</option>
                                            <option value="2">App</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="col-md-4 col-sm-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            FECHA DE INICIO
                                        </span>
                                        <input type="text" class="form-control input-sm dateOnly_" id="fechaIni" readonly />
                                        <span class="input-group-addon input-group-icon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>

                                <div class="col-md-4 col-sm-4">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            FECHA DE FIN
                                        </span>
                                        <input type="text" class="form-control input-sm dateOnly_" id="fechaFin" readonly />
                                        <span class="input-group-addon input-group-icon">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>


                            </div>
                        </div>


                        <div class="col-xs-12 col-md-12 col-sm-12">
                            <hr style="margin-top:10px;" />

                            <div class="col-md-6 col-sm-6" id="rowSala">
                                <div class="input-group ">
                                    <span class="input-group-addon">
                                        Sala
                                    </span>
                                    <select id="cboSala" class="form-control input-sm" name="Sala">
                                        <option value="">--Seleccione--</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6 col-sm-6" id="rowEstado">
                                <div class="input-group ">
                                    <span class="input-group-addon">
                                        Estado
                                    </span>
                                    <select id="cboEstado" class="form-control input-sm" name="Estado">
                                        <option value="">--Seleccione--</option>
                                    </select>
                                </div>
                            </div>

                        </div>

                        <div class="col-md-12">
                            <div class="contenedor_tabla table-responsive">
                                <table id="tableLogApiReniec" class="table table-striped table-bordered table-hover table-condensed"></table>
                            </div>
                        </div>
                        <figure class="highcharts-figure">
                            <div id="parent-container">
                                <div id="play-controls">
                                    <button id="play-pause-button" class="fa fa-play" title="play" style="visibility:hidden"></button>
                                    <input id="play-range" type="range" value="1960" min="1960" max="2018" style="visibility: hidden " />
                                </div>
                                <div id="container"></div>
                            </div>
                            <p class="highcharts-description" style="visibility:hidden">
                                Grafico de barras que muestra la cantidad de usuarios neuvos en el rango de fechas por cada sala.
                            </p>
                        </figure>
                    </div>

                </div>
            </div>
        </div>
    </div>

</div>



@section js
{
    <script src="~/Content/assets/viewsjs/ControlAcceso/ListadoLogApiReniec.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
}