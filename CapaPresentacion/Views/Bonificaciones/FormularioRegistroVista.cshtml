
@{
    ViewBag.Title = "TransferenciaATFormularioRegistro";
}

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <h1>
                                <span aria-hidden="true" class="icon icon-dollar"></span>
                                <span class="main-text">
                                    Formulario Registro Bonificaciones
                                </span>
                            </h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-4 col-md-offset-4 col-sm-5 col-xs-12">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row">

                        <div class="col-md-12 col-sm-12">
                            <div class="form-group">
                                <label for="NroTicket"> Nro Documento</label>
                                <input type="text" class="form-control input-sm " id="nro_documento" name="nro_documento" placeholder="Nro Documento">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <hr style="margin-top: 0px;margin-bottom:10px;" />
                            </div>
                        </div>
                        <div class="col-md-12 col-sm-12">
                            <div class="form-group">
                                <label for="NroTicket">Nro Ticket</label>
                                <input type="text" class="form-control input-sm " id="NroTicket" name="NroTicket" placeholder="Nro Ticket">
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <hr style="margin-top: 0px;" />
                            </div>
                        </div>
                        <div class="col-md-12 col-sm-12">
                            <button class="btn btn-success btn-block" id="btnBuscar"><i class="fa fa-search"></i> Buscar</button>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

</div>



@section js
{
    <script src="~/Content/assets/viewsjs/Bonificaciones/FormularioRegistroVista.js"></script>
}


