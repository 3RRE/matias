@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <div class="col-md-12">
            <form id="formInstancia" role="form">
                <div class="col-md-12 col-sm-12">
                    <div class="input-group form-group">
                        <span class="input-group-addon">
                            Sala
                        </span>
                        <select class="form-control input-sm" id="cboSala" name="CodSala"></select>
                    </div>
                </div>
                <div class="col-md-12 col-sm-12">
                    <div class="input-group form-group">
                        <span class="input-group-addon">
                            Url Base
                        </span>
                        <input type="text" autocomplete="off" class="form-control input-sm" id="UrlBase" name="UrlBase" placeholder="UrlBase de la API de UltraMSG">
                    </div>
                </div>
                <div class="col-md-12 col-sm-12">
                    <div class="input-group form-group">
                        <span class="input-group-addon">
                            Instancia
                        </span>
                        <input type="text" autocomplete="off" class="form-control input-sm" id="Instancia" name="Instancia" placeholder="Instancia proporcionada por UltraMSG">
                    </div>
                </div>
                <div class="col-md-12 col-sm-12">
                    <div class="input-group form-group">
                        <span class="input-group-addon">
                            Token
                        </span>
                        <input type="text" autocomplete="off" class="form-control input-sm" id="Token" name="Token" placeholder="Token proporcionado por UltraMSG">
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

<script src="~/Content/assets/viewsjs/WhatsApp/Instancia/InsertarInstancia.js"></script>
