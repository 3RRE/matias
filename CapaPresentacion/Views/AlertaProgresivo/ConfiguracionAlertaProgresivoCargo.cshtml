@using S3k.Utilitario;

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Configuracion Alerta Progresivo Cargo";

    string fecha_archivos = ClaseEncriptacion.EncriptarCadenaHASH(Convert.ToString(DateTime.Now));
}

<!-- row -->
<div class="row">
    <div class="col-md-12">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <h1>
                                <span aria-hidden="true" class="icon icon-block-menu"></span>
                                <span class="main-text">
                                    @ViewBag.Title
                                </span>
                            </h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /row -->
<!-- row -->
<div class="row">
    <div class="col-md-12">
        <div class="block">
            <div class="block-content-outer">
                <div class="block-content-inner">
                    <div class="row notlabel">
                        <div class="col-md-12">
                            <div>
                                <ul id="tabSala" class="nav nav-tabs tabs-left"></ul>
                                <div class="tab-content" id="tabContentCargo">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /row -->

@section js
{
    <script src="~/Content/assets/viewsjs/Progresivo/Alerta/ConfiguracionAlertaProgresivoCargo.js?v=@fecha_archivos"></script>
}